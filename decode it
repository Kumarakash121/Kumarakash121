 char decodeIt(string str, int k)
    {
        // Write your code here
       


 int n=str.length();
       string s="";
 for(int i=0; i<n; i++)
       {
           if(str[i]>='a' && str[i]<='z')
           {
               s=s+str[i];
         
           }
           else
           {
               int x=str[i]-'0';
              
               string s2="";
               //s2+=s;
               
               while(x--)
               {
                   s2=s2+s;
               }
               s="";
               s+=s2;
               
           }
       }
       return s[k-1];
    
    }
    
    
    
    
